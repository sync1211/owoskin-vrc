<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="OWOVRC.AvaloniaUI.Forms.Dialogs.MessagePopup"
        Title="MessageBox">
        
    <Grid>
		<Label Name="messageLabel" HorizontalAlignment="Left" />
        <Image Name="iconImage" HorizontalAlignment="Left" Width="64" Height="64" />
        
        <Button Name="okButton" Content="OK" Click="OkButton_Click"/>
        <Button Name="cancelButton" Content="Cancel" Click="CancelButton_Click" />
    </Grid>
</Window>
